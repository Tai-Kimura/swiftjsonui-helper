{
  "type": "SafeAreaView",
  "id": "safe_container",
  "width": "matchParent",
  "height": "matchParent",
  "background": "#FFFFFF",
  "child": [
    {
      "data": [
        {
          "name": "userName",
          "class": "String",
          "defaultValue": ""
        },
        {
          "name": "email",
          "class": "String",
          "defaultValue": ""
        },
        {
          "name": "address",
          "class": "String",
          "defaultValue": ""
        }
      ]
    },
    {
      "type": "Scroll",
      "id": "scroll_container",
      "width": "matchParent",
      "height": "matchParent",
      "child": [
        {
          "type": "View",
          "id": "content_view",
          "orientation": "vertical",
          "width": "",
          "height": "wrapContent",
          "child": [
            {
              "type": "Label",
              "id": "text_label",
              "text": "@{serName}",
              "fontSize": 18,
              "fontColor": "#000000",
              "textAlign": "Center",
              "width": "wrapContent",
              "height": "wrapContent"
            }
          ]
        }
      ]
    }
  ]
}
